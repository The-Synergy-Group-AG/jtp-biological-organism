  "success_criteria_definitions": {
    "documentation_coverage": {
      "metric_name": "Documentation Coverage Percentage",
      "description": "Percentage of all docs/ files with biological integration",
      "calculation_method": "scanned_files_with_integration / total_scanable_files * 100",
      "target_threshold": 100.0,
      "validation_method": "BIS_file_audit"
    },
    "metadata_compliance": {
      "metric_name": "Biological Metadata Compliance",
      "description": "Percentage of files with required biological metadata fields",
      "calculation_method": "files_with_complete_metadata / total_files * 100",
      "target_threshold": 100.0,
      "validation_method": "BIS_metadata_scan"
    },
    "cross_reference_accuracy": {
      "metric_name": "Cross-Reference Link Accuracy",
      "description": "Percentage of valid links to strategic master guidance",
      "calculation_method": "valid_links / total_links * 100",
      "target_threshold": 100.0,
      "validation_method": "IEE_link_validation"
    },
    "biological_purity": {
      "metric_name": "Biological Terminology Purity Score",
      "description": "Internal vs external biological terminology separation",
      "calculation_method": "100 - (external_contaminations / total_terminology_instances * 100)",
      "target_threshold": 100.0,
      "validation_method": "BPV_purity_analysis"
    },
    "organizational_consciousness": {
      "metric_name": "Organizational Biological Consciousness",
      "description": "Organization-wide adoption of biological thinking patterns",
      "calculation_method": "team_biological_proficiency_assessments / total_team_members * 100",
      "target_threshold": 100.0,
      "validation_method": "SVD_consciousness_measurement"
    }
  },

